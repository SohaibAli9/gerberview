{
  "name": "@gerberview/web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format:check": "prettier --check \"src/**/*.{ts,js}\"",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage --passWithNoTests",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "jszip": "^3.10"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4",
    "@gerberview/eslint-config": "workspace:*",
    "@playwright/test": "^1",
    "@tailwindcss/vite": "^4",
    "@vitest/coverage-v8": "^3",
    "eslint": "^9",
    "tailwindcss": "^4",
    "typescript": "^5.4",
    "vite": "^6",
    "vite-plugin-top-level-await": "^1",
    "vite-plugin-wasm-pack": "^0.1",
    "vitest": "^3"
  }
}
